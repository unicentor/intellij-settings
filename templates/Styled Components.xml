<templateSet group="Styled Components">
  <template name="sc" value="const $1$ = styled.$2$`&#10;  $END$&#10;`;" description="Styled component" toReformat="false" toShortenFQNames="true">
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="2" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="scg" value="const GlobalStyle = createGlobalStyle`&#10;  $END$&#10;`;" description="createGlobalStyle" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="scc" value="const Styled$1$ = styled($2$)`&#10;  $END$&#10;`;" description="Styled component from existing component" toReformat="false" toShortenFQNames="true">
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="2" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="sca" value="import styled from 'styled-components';&#10;&#10;const $1$ = styled.$2$`&#10;  $END$&#10;`;" description="Styled component atom" toReformat="false" toShortenFQNames="true">
    <variable name="1" expression="fileNameWithoutExtension()" defaultValue="" alwaysStopAt="true" />
    <variable name="2" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="scattrs" value="const $1$ = styled.$2$.attrs({&#10;  $3$: $4$,&#10;})`&#10;  $END$&#10;`;" description="Styled component with attributes" toReformat="false" toShortenFQNames="true">
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="2" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="3" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="4" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="scpt" value="${({ theme }) =&gt; theme.$END$};" description="Use props.theme inside styled-component" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CSS_PROPERTY_VALUE" value="true" />
    </context>
  </template>
  <template name="scp" value="${({ $1$ }) =&gt; $END$};" description="Use props inside styled-component" toReformat="false" toShortenFQNames="true">
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CSS_PROPERTY_VALUE" value="true" />
    </context>
  </template>
  <template name="scpd" value="${({ $propsName$ }) =&gt;&#10;  $propsName$ &amp;&amp;&#10;  css`&#10;    $END$&#10;  `}" description="Conditional CSS inside styled-component" toReformat="false" toShortenFQNames="true">
    <variable name="propsName" expression="complete()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="true" />
      <option name="CSS_PROPERTY_VALUE" value="false" />
    </context>
  </template>
  <template name="scpco" value="${({ theme }) =&gt; theme.colors.$1$};" description="Use props.theme.colors inside styled-component" toReformat="false" toShortenFQNames="true">
    <variable name="1" expression="complete()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CSS_PROPERTY_VALUE" value="true" />
    </context>
  </template>
  <template name="imsc" value="import styled from 'styled-components';" description="import styled from styled-components" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="imscg" value="import { createGlobalStyle } from 'styled-components';" description="import createGlobalStyle from styled-components" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="imsccss" value="import { css } from 'styled-components';" description="import css from styled-components" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="scss" value="const $1$ = css`&#10;  $END$&#10;`;" description="Styled component css" toReformat="false" toShortenFQNames="true">
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="ecss" value="export const css$1$ = css`&#10;  $END$&#10;`;" description="useStyle" toReformat="false" toShortenFQNames="true">
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="css" value="${css$1$};" description="css style" toReformat="false" toShortenFQNames="true">
    <variable name="1" expression="complete()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true" />
      <option name="CSS_PROPERTY_VALUE" value="true" />
    </context>
  </template>
</templateSet>